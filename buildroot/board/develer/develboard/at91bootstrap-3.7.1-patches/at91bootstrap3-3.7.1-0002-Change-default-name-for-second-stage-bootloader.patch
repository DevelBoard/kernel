From a5077f2dd3866a1805bec2eca8ec5b75321969e1 Mon Sep 17 00:00:00 2001
From: Giovanni Bajo <rasky@develer.com>
Date: Thu, 17 Sep 2015 17:09:59 +0200
Subject: [PATCH] Change default name for second-stage bootloader

---
 Config.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Config.in b/Config.in
index 786043c..85bd71b 100644
--- a/Config.in
+++ b/Config.in
@@ -274,7 +274,7 @@ config CONFIG_IMAGE_NAME
 	string
 	default "uImage" if CONFIG_LINUX_UIMAGE
 	default "zImage" if CONFIG_LINUX_ZIMAGE
-	default "u-boot.bin" if CONFIG_LOAD_UBOOT
+	default "barebox.bin" if CONFIG_LOAD_UBOOT
 	default "softpack.bin" if CONFIG_LOAD_64KB || CONFIG_LOAD_4MB || CONFIG_LOAD_1MB
 	  
 config CONFIG_DEBUG
